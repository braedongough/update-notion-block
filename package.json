{
  "name": "update-notion-block",
  "version": "1.0.2",
  "description": "A Github action for updating a block in notion.",
  "main": "index.js",
  "scripts": {
    "build": "ncc build src/index.ts -o dist",
    "exec:local": "ncc run src/index.ts",
    "release": "release-it"
  },
  "author": "Braedon Gough",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.2.6",
    "@notionhq/client": "^2.2.3"
  },
  "devDependencies": {
    "@types/node": "^18.13.0",
    "@vercel/ncc": "^0.36.1",
    "release-it": "^15.6.0",
    "typescript": "^4.9.5"
  },
  "release-it": {
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    }
  }
}
